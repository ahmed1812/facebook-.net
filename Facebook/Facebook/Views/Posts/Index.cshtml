@*@model IEnumerable<Facebook.Models.Post>
*@
@model IEnumerable<Facebook.Models.UserPostViewModel>
@{
    ViewData["Title"] = "Index";
}
@foreach (var item in Model)
{
            
        @Html.DisplayFor(modelItem => item.MyInfo.FullName)<br />
            

        @foreach (var p in item.UserPosts)
        {
                @Html.DisplayFor(modelItem => p.Titel)<br />
                @Html.DisplayFor(modelItem => p.Body)<br />
        @foreach (var c in item.comments)
        {
            @Html.DisplayFor(modelItem => c.Body)
            <br />
        }

}<br />
           
}<br />